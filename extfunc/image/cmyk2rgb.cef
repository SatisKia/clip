#!NAMESPACE image
#CMYKからRGBへの変換
#\!- <c_array> <m_array> <y_array> <k_array> <rgb_array>
:ans FALSE
:param 4 TRUE

$COL		@w,@@0
$ROW		@h,@@0
$SET_Z		@y
$LOOPSTART	# @y
$SET_Z		@x
$LOOPSTART	# @x
$DIV_A		@C,@@0 @y @x,255
$DIV_A		@M,@@1 @y @x,255
$DIV_A		@Y,@@2 @y @x,255
$DIV_A		@K,@@3 @y @x,255

$MUL_A		@R,(1 - @C),(1 - @K)
$MUL_A		@G,(1 - @M),(1 - @K)
$MUL_A		@B,(1 - @Y),(1 - @K)

$MKCOLORS	@@4 @y @x,(int (@R * 255)),(int (@G * 255)),(int (@B * 255))
$LOOPEND_I	@x,@w
$LOOPEND_I	@y,@h
