#輪郭抽出その2
case 4; @@m{{0 1 0}{1 \-4 1}{0 1 0}}; breakswi
#エンボス
case 5; @@m{{\-1 0 0}{0 0 0}{0 0 1}}; breakswi
#エンボスその2
case 6; @@m{{\-2 \-1 0}{\-1 1 1}{0 1 2}}; breakswi
