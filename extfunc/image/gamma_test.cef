func gamma
	return (@1 / 255) ** (1 / @0) * 255
endfunc

:include !tplot

:ans FALSE
:gworld 256 256

window 0 0 255 255 32 32

plot ["gamma (1/4) x] 1   25 @y
plot ["gamma (1/3) x] 2   25 @y
plot ["gamma (1/2) x] 3   25 @y
plot ["gamma 1 x]     4   25 @y
plot ["gamma 2 x]     254 25 @y
plot ["gamma 3 x]     253 25 @y
plot ["gamma 4 x]     252 25 @y
