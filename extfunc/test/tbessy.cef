	plot ["!y 1 x] 2 160 @y
	plot ["!y 2 x] 3 160 @y
	plot ["!y 3 x] 4 160 @y
else
	plot ["!bessy0 x]  1 160 @y
	plot ["!bessy1 x]  2 160 @y
	plot ["!bessy 2 x] 3 160 @y
	plot ["!bessy 3 x] 4 160 @y
